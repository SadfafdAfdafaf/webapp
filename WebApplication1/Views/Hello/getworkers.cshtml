@model IEnumerable<WebApplication1.Models.personalinfmodel>

@{
    ViewBag.Title = "regions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>REGIONS</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.claster)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.claster)
            </td>
        </tr>
    }

</table>
