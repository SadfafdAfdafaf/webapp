@using PagedList.Mvc;
@model PagedList.IPagedList<WebApplication1.Models.personalinfmodel>


<link href="~/Content/PagedList.css" rel="stylesheet" type="text/css" />

@{
    ViewBag.Title = "getregionspage";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>getregionspage</h2>

<table class="table">
    <tr>
        <th>
            Id
        </th>
        <th>
            claster
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.claster)
            </td>

        </tr>
    }

</table>

Страница @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) из @Model.PageCount

@Html.PagedListPager(Model, page => Url.Action("getcompaniespage", new { page }))