@model IEnumerable<WebApplication1.Models.detailedworkermodel>

@{
    ViewBag.Title = "getfilteredworkers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>getfilteredworkers</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FIO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CEO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.region)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cost)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegionOffice)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FIO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CEO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.region)
            </td>
            <th>
                @Html.DisplayFor(modelItem => item.Cost)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.RegionOffice)
            </th>
        </tr>
    }

</table>